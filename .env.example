# Docker Compose Environment Variables
# Copy this file to .env and adjust values as needed

# Application
APP_NAME=SWStarter
APP_ENV=local
APP_DEBUG=true

# Ports (change if you need different ports)
FRONTEND_PORT=5173
BACKEND_PORT=8000
REDIS_PORT=6379

# Redis Configuration
REDIS_HOST=redis
CACHE_DRIVER=redis
QUEUE_CONNECTION=redis

# External API
SWAPI_BASE_URL=https://swapi.dev/api

# Frontend Configuration
VITE_API_URL=http://localhost:8000
